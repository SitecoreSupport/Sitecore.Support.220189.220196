        <processor type="Sitecore.Modules.EmailCampaign.Core.Pipelines.HandleMessageEventBase.SetCustomValues, Sitecore.EmailCampaign">
          <patch:attribute name="type">
            Sitecore.Support.Modules.EmailCampaign.Core.Pipelines.HandleMessageEventBase.SetCustomValues, Sitecore.Support.220189.220196
